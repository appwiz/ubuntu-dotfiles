alias ps='ps auxww'
alias gs='git status'
alias gl='git log'
alias gri='git rebase -i'
alias gpr='git pull --rebase'
alias gcl='git clone'
alias gba='git branch -a'
alias mkdir='mkdir -pv'
alias wget='wget -c'
alias cls='clear'
